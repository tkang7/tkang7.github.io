---
import BaseLayout from "../layouts/BaseLayout.astro";
import TimeLineElement from "../components/cv/TimeLine.astro";
---

<BaseLayout title="Resume" sideBarActiveItemID="cv">
  <div class="mb-5">
    <div class="text-3xl w-full font-bold">Profile</div>
  </div>

  <div class="mb-10 text-justify">
    I have 2+ years of industry experience working as a Software Engineer. <br
    />
    I have 1+ years of experience implementing and deploying Machine Learning Models.
    <br />
    I have 5+ years of experience managing organizations and projects.
    <br />
    <a href="/public/Taekyu_Kang_Resume.pdf" download
      ><u>Click here to download my resume</u></a
    >
  </div>

  <div class="mb-5">
    <div class="text-3xl w-full font-bold">Education</div>
  </div>

  <div class="time-line-container grid gap-4 mb-10">
    <TimeLineElement
      title="University of British Columbia"
      subtitle="Master of Data Science in Computational Linguistics (NLP)"
      detail="Expected June 2025"
    />
    <TimeLineElement
      title="University of British Columbia"
      subtitle="Bachelor of Applied Science in Computer Engineering, Minor in Commerce"
      detail="Graduated May 2022"
    />
  </div>

  <div class="mb-5">
    <div class="text-3xl w-full font-bold">Recent Experience</div>
  </div>

  <div class="time-line-container mb-10">
